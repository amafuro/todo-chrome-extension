{
  "manifest_version": 3,
  "name": "欲しかったTODOリスト",
  "version": "1.0.0",
  "description": "親子関係を持つTODOをガントチャートで管理できる拡張機能",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/memo-icon.png",
      "48": "icons/memo-icon.png",
      "128": "icons/memo-icon.png"
    }
  },
  "icons": {
    "16": "icons/memo-icon.png",
    "48": "icons/memo-icon.png",
    "128": "icons/memo-icon.png"
  }
}

